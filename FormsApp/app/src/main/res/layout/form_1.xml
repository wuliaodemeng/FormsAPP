<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFAFBFB"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="44dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="0dp">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#FFFFFFFF"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light">
            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageButton
                    android:layout_width="15.7dp"
                    android:layout_height="11.3dp"
                    android:background="#FF9DA3B4"
                    android:src="@drawable/return_back"
                    android:id="@+id/image_button"

                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginTop="16dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    android:layout_marginRight="350dp"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginBottom="16dp"
                    app:layout_constraintVertical_bias="0.0"
                    app:layout_constraintHorizontal_bias="0.571" />
                
                <TextView
                    android:layout_width="74dp"
                    android:layout_height="19dp"
                    android:text="企业信息"
                    android:textColor="#FF454553"
                    android:textSize="16sp"
                    app:layout_constraintRight_toRightOf="parent"
                    android:layout_marginRight="164dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginBottom="12dp" />

            </android.support.constraint.ConstraintLayout>

        </android.support.v7.widget.Toolbar>
    </FrameLayout>

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.constraint.ConstraintLayout
            android:layout_width="340dp"
            android:layout_height="56dp"
            android:background="#FFFFFFFF"

            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="28dp"
            android:layout_marginLeft="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="10dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="493dp"
            app:layout_constraintVertical_bias="0.0">

            <TextView
                android:layout_width="72dp"
                android:layout_height="17dp"
                android:text="企业名称："
                android:textColor="#FF878787"
                android:textSize="14sp"

                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="19dp"
                android:layout_marginLeft="25dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginRight="243dp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="20dp"
                app:layout_constraintVertical_bias="0.0"
                android:id="@+id/textView" />

            <EditText
                android:layout_width="214dp"
                android:layout_height="32dp"
                android:background="@null"
                android:id="@+id/editText1"
                android:layout_marginRight="16dp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" />


        </android.support.constraint.ConstraintLayout>

        <android.support.constraint.ConstraintLayout
            android:layout_width="340dp"
            android:layout_height="56dp"
            android:background="#FFFFFFFF"

            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="94dp"
            android:layout_marginLeft="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="10dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="427dp"
            app:layout_constraintVertical_bias="0.0">

            <TextView
                android:layout_width="72dp"
                android:layout_height="17dp"
                android:text="企业地址："
                android:textColor="#FF878787"
                android:textSize="14sp"

                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="19dp"
                android:layout_marginLeft="25dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginRight="243dp"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="20dp"
                app:layout_constraintVertical_bias="0.0"/>
            <EditText
                android:layout_width="214dp"
                android:layout_height="32dp"
                android:background="@null"
                android:id="@+id/editText2"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginRight="16dp"
                app:layout_constraintRight_toRightOf="parent"/>
            </android.support.constraint.ConstraintLayout>


        <Button
            android:id="@+id/button"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="197dp"
            android:layout_marginLeft="80dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="80dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="338.1dp"
            app:layout_constraintVertical_bias="0.0"

            android:text="下一步"
            android:textColor="#FFFFFFFF"
            android:textSize="14sp"
            android:layout_width="200dp"
            android:layout_height="41.9dp"
            android:background="#FF1BE58D"
            android:gravity="center"/>


    </android.support.constraint.ConstraintLayout>


</LinearLayout>
